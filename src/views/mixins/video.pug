mixin video(video)
    div 
        h2
            a(href=`/videos/${video.id}` )=video.title
        small=video.createdAt.toLocaleDateString("ko-kr", {weekday: 'long', year: 'numeric', month: 'long', day: 'numeric'})
        p=video.description
        ul
            each hashtag in video.hashtags
                li=hashtag 
        hr